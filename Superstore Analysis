-- Order items by price.
SELECT item_name, price
FROM superstore
ORDER BY price DESC;
-- Find the average price of all items in the superstore. 
SELECT AVG(price)
FROM superstore;
-- Find the maximum price of all items within the Kitchen Supplies category. 
SELECT MAX(price),'Kitchen Supplies'
FROM superstore
GROUP BY'Kitchen Supplies';
-- Find which items have a stock quantity of more than 50.
SELECT item_name
FROM superstore
WHERE stock_quantity>50;
